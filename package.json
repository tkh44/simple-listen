{
  "name": "simple-listen",
  "version": "0.1.0",
  "description": "Utility for adding event listeners with simple unsubscribing",
  "main": "dist/simple-listen.umd.js",
  "jsnext:main": "dist/simple-listen.es6.js",
  "scripts": {
    "prebuild": "eslint lib test",
    "build": "rollup -c rollup.config.umd.js && rollup -c rollup.config.es6.js",
    "pretest": "rollup -c rollup.config.test.js",
    "test": "mocha build/test-bundle.js",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkh44/simple-listen.git"
  },
  "keywords": [
    "es6",
    "event",
    "listener",
    "event listener",
    "browser"
  ],
  "author": "Kye Hohenberger",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/tkh44/simple-listen/issues"
  },
  "files": [
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/tkh44/simple-listen#readme",
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015-rollup": "^1.1.1",
    "code": "^2.1.1",
    "eslint": "^1.10.3",
    "mocha": "^2.3.4",
    "rollup": "^0.24.1",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-multi-entry": "^1.1.0",
    "source-map-support": "^0.4.0"
  }
